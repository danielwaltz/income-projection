{
  "name": "income",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --modern",
    "surge": "surge --project ./dist --domain income-projection.surge.sh",
    "deploy": "run-s build surge"
  },
  "dependencies": {
    "currency-formatter": "^1.5.3",
    "vue": "^2.5.22"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "surge": "^0.20.1",
    "vue-template-compiler": "^2.5.22"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
